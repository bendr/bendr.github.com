<component xmlns="http://bender.igel.co.jp">
  <property name="x" type="number" value="200"/>
  <property name="y" type="number" value="100"/>
  <property name="w" type="number" value="300"/>
  <property name="h" type="number" value="150"/>
  <property name="color" value="#ff4040"/>
  <view xmlns:svg="http://www.w3.org/2000/svg">
    <svg:svg>
      <svg:rect x="{x}" y="{y}" height="{h}" width="{w}" fill="{color}"/>
    </svg:svg>
  </view>
</component>
